{
  "name": "@donkeylabs/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "CLI for @donkeylabs/server - project scaffolding and code generation",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "donkeylabs": "./src/index.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9"
  },
  "peerDependencies": {
    "@donkeylabs/server": "*"
  },
  "keywords": [
    "cli",
    "donkeylabs",
    "scaffolding",
    "code-generation",
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeylabs/server",
    "directory": "packages/cli"
  },
  "license": "MIT"
}
