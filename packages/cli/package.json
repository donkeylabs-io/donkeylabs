{
  "name": "@donkeylabs/cli",
  "version": "1.1.4",
  "type": "module",
  "description": "CLI for @donkeylabs/server - project scaffolding and code generation",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "donkeylabs": "./src/index.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "kysely": "^0.27.0",
    "kysely-bun-sqlite": "^0.3.0",
    "kysely-codegen": "^0.17.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "sharp": "^0.33.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9"
  },
  "peerDependencies": {
    "@donkeylabs/server": "^1.0.0"
  },
  "keywords": [
    "cli",
    "donkeylabs",
    "scaffolding",
    "code-generation",
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeylabs-io/donkeylabs",
    "directory": "packages/cli"
  },
  "license": "MIT"
}
