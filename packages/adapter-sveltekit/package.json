{
  "name": "@donkeylabs/adapter-sveltekit",
  "version": "1.1.16",
  "type": "module",
  "description": "SvelteKit adapter for @donkeylabs/server - seamless SSR/browser API integration",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts"
    },
    "./generator": {
      "types": "./src/generator/index.ts",
      "import": "./src/generator/index.ts"
    },
    "./vite": {
      "types": "./src/vite.ts",
      "import": "./src/vite.ts"
    }
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "typecheck": "bun --bun tsc --noEmit"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@donkeylabs/server": "^1.0.0"
  },
  "keywords": [
    "sveltekit",
    "svelte",
    "adapter",
    "api",
    "rpc",
    "ssr",
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeylabs-io/donkeylabs",
    "directory": "packages/adapter-sveltekit"
  },
  "license": "MIT"
}