{
  "name": "@donkeylabs/adapter-sveltekit",
  "version": "2.0.15",
  "type": "module",
  "description": "SvelteKit adapter for @donkeylabs/server - seamless SSR/browser API integration",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./generator": {
      "types": "./dist/generator/index.d.ts",
      "import": "./dist/generator/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@donkeylabs/server": "^2.0.6"
  },
  "keywords": [
    "sveltekit",
    "svelte",
    "adapter",
    "api",
    "rpc",
    "ssr",
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeylabs-io/donkeylabs",
    "directory": "packages/adapter-sveltekit"
  },
  "license": "MIT"
}
